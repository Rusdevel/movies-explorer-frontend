{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      { "allow": ["_id", "foo_"], "allowAfterThis": true }
    ],
    // "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "no-console": 0
  }
}
